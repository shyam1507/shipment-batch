<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.shipmentbatch.mapper.ShipmentMapper">
    <insert id="insertShipment" parameterType="com.example.shipmentbatch.model.Shipment">
        INSERT INTO shipment (order_id, customer_name, address, status)
        VALUES (#{orderId}, #{customerName}, #{address}, #{status});
    </insert>
</mapper>